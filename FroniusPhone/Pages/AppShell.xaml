<?xml version="1.0" encoding="UTF-8"?>

<Shell x:Class="FroniusPhone.Pages.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:p="clr-namespace:De.Hochstaetter.Fronius.Localization;assembly=Fronius"
       Shell.FlyoutBehavior="Disabled"
       BindingContext="{Binding ViewModel, Source={RelativeSource Self}}">

    <ShellContent x:Name="ShellContent"
                  Title="{x:Static p:Resources.FroniusManager}"
                  Route="MainPage" />

</Shell>